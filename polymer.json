{
  "entrypoint": "index.html",
  "shell": "src/memeservice.html",
  "fragments": [
    "src/upload.html",
    "src/admin.html",
    "src/view404.html",
    "src/dowork.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
